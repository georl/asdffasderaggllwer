stock PrintBizInfo(playerid,targetid)
{
	if(IsPlayerConnected(playerid))
	{
		new mafiatext[20],stringer[144];
		if(BizzInfo[targetid][bMafia] == 6) { mafiatext = "Yakuza"; }
		else if(BizzInfo[targetid][bMafia] == 14) { mafiatext = "RM"; }
		else if(BizzInfo[targetid][bMafia] == 5) { mafiatext = "LCN"; }
		else if(BizzInfo[targetid][bMafia] == 0) { mafiatext = "Нет"; }
		new elektrich = BizzInfo[targetid][bBuyPrice] / 1000;
		new text1[20],text2[20];
		if(BizzInfo[targetid][bLocked]) { text1 = "Закрыт"; } else { text1 = "Открыт"; }
		if(PlayerInfo[playerid][pBizLic]) { text2 = "Есть"; } else { text2 = "Отсутсвует"; }
		format(stringer,sizeof(stringer),"{FFFFFF}Бизнес: %s\nНа счету бизнеса: %d вирт\nВ этом часу заработано: %d вирт\nЦена входа: %d вирт\nПлата за электричество: %d вирт\nЛицензия на бизнес: %s\nЦена за товар: %d\nКрыша: %s \n\nСостояние открытия: %s\nПродукты: %d / 100000",
		BizzInfo[targetid][bMessage],BizzInfo[targetid][bTill],BizzInfo[targetid][b2Till],BizzInfo[targetid][bEntranceCost],elektrich,text2,BizzInfo[targetid][bCena],mafiatext,text1,BizzInfo[targetid][bProducts]);
		SPD(playerid,10349,DIALOG_STYLE_MSGBOX,"Cостояние бизнеса",stringer,"Ок","");
	}
}
stock UpdateSBizz()
