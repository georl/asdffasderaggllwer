				SendClientMessage(MarryWitnessOffer[playerid], 0x6ab1ffaa, strcmd);
				MarryWitness[MarryWitnessOffer[playerid]] = playerid;
				MarryWitnessOffer[playerid] = INVALID_PLAYER_ID;
				return true;
			}
			else return SendClientMessage(playerid, COLOR_GREY, "Игрок, который просил, чтобы вы были свидетелем не рядом с вами !");
		}
		else return SendClientMessage(playerid, COLOR_GREY, "Вам не предлагали быть свидетелем!");
	}
	else if(strcmp(params[0],"fight",true) == 0)
	{
		if(fightid[playerid] < 176 && boy[playerid] == 0)
		{
			if(ring == 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "Боксёрский ринг занят.");
			new healthh = 30+ (PlayerInfo[playerid][pFightSkill]*10);
			new healthhg = 30+ (PlayerInfo[fightid[playerid]][pFightSkill]*10);
			boy[playerid] = 1;
			boy[fightid[playerid]] = 1;
			SetPlayerHealthAC(playerid, healthh);
			SetPlayerHealthAC(fightid[playerid], healthhg);
			TogglePlayerControllable(playerid, 0);
			TogglePlayerControllable(fightid[playerid], 0);
			SetPPos(playerid,758.7978,-1.7495,1001.5942);
			SetPPos(fightid[playerid],762.9683,2.4081,1001.5942);
			SetPlayerFacingAngle(playerid, 319.6382);
			SetPlayerFacingAngle(fightid[playerid], 137.3811);
			format(strcmd,sizeof(strcmd), " ~g~%d HP", healthh);
			GameTextForPlayer(playerid, strcmd, 3000, 1);
			format(strcmd,sizeof(strcmd), " ~g~%d HP", healthhg);
			GameTextForPlayer(fightid[playerid], strcmd, 3000, 1);
			ring = 1;
			TimeBox[playerid] = 10;
			TimeBoxAll[playerid] = 600;
			TimeBox[fightid[playerid]] = 10;
			TimeBoxAll[fightid[playerid]] = 600;
			SendClientMessage(playerid, COLOR_LIGHTRED, "Используйте свои навыки драки, чтобы победить противника.");
			SendClientMessage(fightid[playerid], COLOR_LIGHTRED, "Используйте свои навыки драки, чтобы победить противника.");
		}
	}
	else if(strcmp(params[0],"marriage",true) == 0)
	{
		if(ProposeOffer[playerid] != INVALID_PLAYER_ID)
		{
			if(!PlayerToPoint(10.0, playerid, -1988.6638,1117.8837,54.4726)) return   SendClientMessage(playerid, COLOR_GREY, "Вы не у церкви в Сан-Фиерро!");
			if(ProxDetectorS(10.0, playerid, ProposeOffer[playerid]))
			{
				if(MarryWitness[ProposeOffer[playerid]] == INVALID_PLAYER_ID) return      SendClientMessage(playerid, COLOR_GREY, "У него/неё нет свидетеля!");
				if(ProxDetectorS(12.0, ProposeOffer[playerid], MarryWitness[ProposeOffer[playerid]]))
				{
					format(strcmd,sizeof(strcmd), "Вы приняли предложение от %s's", PlayerInfo[ProposeOffer[playerid]][pNames]);
					SendClientMessage(playerid, 0x6ab1ffaa, strcmd);
					format(strcmd,sizeof(strcmd), "%s приняла Ваш запрос быть Вашей Женой.", PlayerInfo[playerid][pNames]);
					SendClientMessage(ProposeOffer[playerid], 0x6ab1ffaa, strcmd);
					format(strcmd,sizeof(strcmd), "%s.ini",PlayerInfo[ProposeOffer[playerid]][pNames]);
					new files = ini_openFile(strcmd);
					if(files < 0) print("нету файла");
					else
					{
						ini_setString(files,"MarriedTo",PlayerInfo[playerid][pNames]);
						ini_closeFile(files);
					}
					format(strcmd,sizeof(strcmd), "{FC6DEC}[Зарегистрирован новый брак] %s с %s", PlayerInfo[playerid][pNames], PlayerInfo[ProposeOffer[playerid]][pNames]);
					SendClientMessageToAll(COLOR_WHITE,strcmd);
					PlayerInfo[ProposeOffer[playerid]][pCash] -= 100000;
					PlayerInfo[playerid][pMarried] = 1;
					PlayerInfo[ProposeOffer[playerid]][pMarried] = 1;
					MarryWitness[ProposeOffer[playerid]] = INVALID_PLAYER_ID;
					ProposeOffer[playerid] = INVALID_PLAYER_ID;
				}
			}
			else return SendClientMessage(playerid, COLOR_GREY, "Жених/Невеста не рядом с Вам!");
		}
		else return SendClientMessage(playerid, COLOR_GREY, "Вам ни кто не предлагал!");
	}
	else if(strcmp(params[0],"taxi",true) == 0)
	{
		if(TransportDuty[playerid] != 1) return   SendClientMessage(playerid, COLOR_GREY, "Вы не таксист!");
		if(TaxiCall != INVALID_PLAYER_ID)
		{
			format(strcmd,sizeof(strcmd), "Вы приняли вызов от %s.",PlayerInfo[TaxiCall][pNames]);
			SendClientMessage(playerid, 0x6ab1ffaa, strcmd);
			format(strcmd,sizeof(strcmd), "Водитель такси %s принял ваш вызов, оставайтесь на месте.",PlayerInfo[playerid][pNames]);
