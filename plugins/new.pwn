	if(GetPVarInt(playerid,"AntiFlood") > gettime())
	{
		SendClientMessage(playerid, 0xFFD5BBAA, "Не флуди!");
		return 0;
	}
	SetPVarInt(playerid,"AntiFlood",gettime() + 1);
	return 1;
}
