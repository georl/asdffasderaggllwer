			TaxiCall = INVALID_PLAYER_ID;
			return true;
		}
		else return SendClientMessage(playerid, COLOR_GREY, "Нет Вызовов");
	}
	else if(strcmp(params[0],"ticket",true) == 0)
	{
		if(TicketOffer[playerid] != INVALID_PLAYER_ID)
		{
			if (ProxDetectorS(5.0, playerid, TicketOffer[playerid]))
			{
				format(strcmd,sizeof(strcmd), "Вы оплатили штраф в размере %d вирт офицеру %s.", TicketMoney[playerid], PlayerInfo[TicketOffer[playerid]][pNames]);
				SendClientMessage(playerid, COLOR_BLUE, strcmd);
				format(strcmd,sizeof(strcmd), "%s оплатил штраф в размере %d вирт.", PlayerInfo[playerid][pNames], TicketMoney[playerid]);
				SendClientMessage(TicketOffer[playerid], COLOR_BLUE, strcmd);
				PlayerInfo[playerid][pCash] -=TicketMoney[playerid];
				PlayerInfo[TicketOffer[playerid]][pCash] +=TicketMoney[playerid];
				TicketOffer[playerid] = INVALID_PLAYER_ID;
				TicketMoney[playerid] = 0;
				return true;
			}
			else return SendClientMessage(playerid, COLOR_GREY, "Полицейскйи не рядом с вами!");
		}
		else return SendClientMessage(playerid, COLOR_GREY, "Вам не выписывали штраф!");
	}
	else if(strcmp(params[0],"drugs",true) == 0)
	{
		if(DrugOffer[playerid] != INVALID_PLAYER_ID)
		{
			if(PlayerInfo[playerid][pCash] > DrugPrice[playerid])
			{
				if(PlayerInfo[playerid][pDrugs] < 7)
				{
					format(strcmd,sizeof(strcmd), "Вы купили %d грамм наркотиков за %d вирт у %s.",DrugGram[playerid],DrugPrice[playerid],PlayerInfo[DrugOffer[playerid]][pNames]);
