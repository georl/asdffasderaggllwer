	new stringer[420];
	for(new b = 0; b < sizeof(SBizzInfo); b++)
	{
		Delete3DTextLabel(SBizzInfo[b][sLabel]);
		DestroyDynamicMapIcon(SBizzInfo[b][sIcon]);
		if(SBizzInfo[b][sOwned] == 0) SBizzInfo[b][sLocked] = 1,format(stringer,sizeof(stringer), "{FFFF00}%s\n{FFFFFF}Цена: {32CD32}%d{FFFFFF} вирт\n\n\n[{FF0000}Заправка не работает{FFFFFF}]\nВведите: {ffff00}[/buyfuel] {ffffff}что бы купить заправку",SBizzInfo[b][sMessage],SBizzInfo[b][sBuyPrice]);
		if(SBizzInfo[b][sOwned] == 1)
