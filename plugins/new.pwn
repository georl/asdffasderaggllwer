stock ShowPlayerMembers(playerid)
{
	new stringer[1024];
	new teamnumber;
	if (PlayerInfo[playerid][pLRInections] != 0) teamnumber = PlayerInfo[playerid][pLRInections];
	else if (PlayerInfo[playerid][pMRInections] != 0) teamnumber = PlayerInfo[playerid][pMRInections];
	else return SendClientMessage(playerid, COLOR_GRAD1, "Вы ни где не состоите!");
	SendClientMessage(playerid, 0x059BD3AA, "Члены организации {ff5000}Online:");
	new countmute = 0;
	foreach(new i : Player)
	{
		if (PlayerInfo[i][pMRInections] == teamnumber && IsPlayerConnected(i))
		{
			format(stringer,sizeof(stringer), "");
			countmute++;
			if (PlayerInfo[i][pLRInections] == teamnumber) format(stringer,sizeof(stringer), "{F2EEF0}[%d] %s ранг: {e6e6fa}Руководитель {FFFFFF}", i, PlayerInfo[i][pNames]);
			else if (PlayerInfo[i][pMRInections] == teamnumber) format(stringer,sizeof(stringer), "{F2EEF0}[%d] %s ранг: {e6e6fa}%d {FFFFFF}", i, PlayerInfo[i][pNames], PlayerInfo[i][pRKInections]);
			if (strlen(stringer) > 1) SendClientMessage(playerid, 0xC0E1EEFF, stringer);
		}
	}
	if(countmute == 0) SendClientMessage(playerid, COLOR_GRAD1, "{00C0FF}В данный момент, все игроки состоящие во фракции {ff0000}OFFLINE");
	else { SendMes(playerid, COLOR_WHITE, "Численность данной фракции: {ff5000}%d {ffffff}человек.", countmute); }
	return true;
}
