					format(strcmd,sizeof(strcmd), "%s купил у вас %d грамм наркотиков за %d вирт",PlayerInfo[playerid][pNames],DrugGram[playerid],DrugPrice[playerid]);
					SendClientMessage(DrugOffer[playerid], 0x6ab1ffaa, strcmd);
					PlayerInfo[playerid][pCash] -=DrugPrice[playerid];
					GivePlayerMoney(DrugOffer[playerid], DrugPrice[playerid]);
					PlayerInfo[playerid][pDrugs] += DrugGram[playerid];
					PlayerInfo[DrugOffer[playerid]][pDrugs] -= DrugGram[playerid];
					return true;
				}
				else return SendClientMessage(playerid, COLOR_GREY, "У Вас и так достаточно наркотиков!");
			}
			else return SendClientMessage(playerid, COLOR_GREY, "У вас нет столько денег!");
		}
		else return SendClientMessage(playerid, COLOR_GREY, "Вам никто не предлагал купить наркотики!");
	}
	else if(strcmp(params[0],"vehicle",true) == 0)
	{
