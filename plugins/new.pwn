			if(PlayerInfo[playerid][pMRInections] == 24 || PlayerInfo[playerid][pLRInections] == 24) GiveWeapon(playerid, 5, 1);
			if(PlayerInfo[playerid][pMRInections] == 6 || PlayerInfo[playerid][pLRInections] == 6) GiveWeapon(playerid, 8, 1);
			if(PlayerInfo[playerid][pMRInections] == 7 && PlayerInfo[playerid][pRKInections] == 3) GiveWeapon(playerid, 24, 100),SetPlayerArmourAC(playerid, 100);
